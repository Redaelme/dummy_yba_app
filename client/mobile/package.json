    "ios": "react-native run-ios",
    "android:reverse": "adb reverse tcp:4000 tcp:4000 && adb reverse tcp:8081 tcp:8081 && adb reverse tcp:8097 tcp:8097 && adb reverse tcp:9090 tcp:9090 && adb reverse tcp:5051 tcp:5051",
